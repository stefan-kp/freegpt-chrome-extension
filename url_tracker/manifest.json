{
  "manifest_version": 3,
  "name": "FreeGPT",
  "version": "1.2",
  "permissions": [
    "tabs",
    "webNavigation",
    "scripting",
    "storage",
    "windows"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "host_permissions": ["<all_urls>"],
  "options_page": "options.html",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}