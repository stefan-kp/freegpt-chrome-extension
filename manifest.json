{
  "manifest_version": 3,
  "name": "FreeGPT",
  "version": "1.2",
  "permissions": [
    "tabs",
    "webNavigation",
    "scripting",
    "storage",
    "windows"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "free_gpt/youtube_parse_start.js",
        "free_gpt/youtube_parse_history.js",
        "free_gpt/youtube_video_merge.js"
      ],
      "matches": ["https://www.youtube.com/*"]
    }
  ]
}